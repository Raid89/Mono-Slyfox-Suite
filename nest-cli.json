{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "prisma/schema.prisma",
        "outDir": "dist"
      },
      {
        "include": "templates/verify-email.html",
        "outDir": "dist"
      }
    ]
  }
}
